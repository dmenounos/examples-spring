<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %-5p [%-25t] %-40c{40} : %m%n</pattern>
		</encoder>
	</appender>

	<logger name="com.example.websocket" level="all" />

	<root level="warn">
		<appender-ref ref="stdout" />
	</root>

</configuration>
